<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fullfit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Fitness Center"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FullFit">
        <activity
            android:name=".NewsDescriptionActivity"
            android:exported="true"
            android:label="Details"/>
        <activity
            android:name=".MeasureActivity"
            android:exported="true"
            android:label="Measure Your Fitness" />
        <activity
            android:name=".NewsActivity"
            android:exported="true"
            android:label="Fitness News" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>